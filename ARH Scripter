    "ğŸª› â€¢ " .. _("drill_matdung"),
    "âŒ â€¢ " .. _("back_matdung")
  }, nil, indev)

  if not menu then
    return
  end

  local actions = {
    gekom1, gekom2, gekom3, gekom4, gekom5, gekom6
  }

  if menu >= 1 and menu <= 6 then
    actions[menu](caller)
