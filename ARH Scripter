  {-0x3C, 0x8EFCC1BB}, {-0x38, 0x23330000}, {-0x34, 0x6F},
  {-0x30, 0}, {-0x2C, "amount"}, {0x10, 0}, {0x18, 0}, {0x20, 0}
}, caller) end

