  end
  table.insert(edits, {address = addr + 0x10, flags = gg.TYPE_DWORD, value = 0})
  table.insert(edits, {address = addr + 0x18, flags = gg.TYPE_DWORD, value = 0})
  table.insert(edits, {address = addr + 0x20, flags = gg.TYPE_DWORD, value = 0})
